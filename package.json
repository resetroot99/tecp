{
  "name": "tecp-protocol",
  "version": "0.1.0",
  "description": "Trusted Ephemeral Computation Protocol - Making privacy violations mathematically impossible",
  "private": true,
  "scripts": {
    "postinstall": "npm run install:workspaces",
    "install:workspaces": "cd packages/tecp-reference-ui && npm install",
    "build": "npm run build:core && npm run build:verifier && npm run build:log && npm run build:ui && npm run build:sdks",
    "build:sdks": "npm run build:sdk-js",
    "build:sdks:all": "npm run build:sdk-js && npm run build:sdk-py && npm run build:sdk-go",
    "build:core": "tsc -p packages/tecp-core/",
    "build:verifier": "tsc -p packages/tecp-verifier/",
    "build:log": "tsc -p services/tecp-log/",
    "build:ui": "cd packages/tecp-reference-ui && npm run build",
    "build:sdk-js": "tsc -p packages/tecp-sdk-js/",
    "build:sdk-py": "cd packages/tecp-sdk-py && python setup.py build",
    "build:sdk-go": "cd packages/tecp-sdk-go && go build ./...",
    "test": "npm run test:vectors && npm run test:interop && npm run test:policy-registry",
    "test:vectors": "node scripts/test-vectors.js",
    "test:interop": "node scripts/test-interop.js",
    "test:fuzz": "node scripts/fuzz-test.js",
    "test:policy-registry": "node scripts/validate-policy-registry.js",
    "dev:demo": "cd demo/private-gpt && npm run dev",
    "dev:log": "cd services/tecp-log && npm run dev",
    "dev:verifier": "cd packages/tecp-verifier && npm run dev",
    "dev:ui": "cd packages/tecp-reference-ui && npm run dev",
    "dev:all": "concurrently \"npm run dev:demo\" \"npm run dev:log\" \"npm run dev:verifier\" \"npm run dev:ui\"",
    "start:reference": "node scripts/start-reference.js",
    "start:week2": "node scripts/start-week2.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "gen:keys": "node scripts/gen-keys.js",
    "verify": "node packages/tecp-verifier/dist/cli.js",
    "clean": "rm -rf node_modules packages/*/dist demo/*/dist services/*/dist"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tecp-protocol/tecp.git"
  },
  "keywords": [
    "privacy",
    "ephemeral-computing",
    "cryptographic-receipts",
    "zero-trust",
    "compliance",
    "gdpr",
    "hipaa"
  ],
  "license": "Apache-2.0",
  "author": "Ali Jakvani",
