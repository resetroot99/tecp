    "@noble/curves": "^1.6.0",
    "@noble/ed25519": "^2.3.0",
    "@noble/hashes": "^1.8.0",
    "cbor-x": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "sqlite3": "^5.1.6"
  },
  "tecp": {
