{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "packages/tecp-reference-ui/dist",
  "builds": [
    {
      "src": "packages/tecp-reference-ui/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/ledger.js",
      "use": "@vercel/node@3.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/ledger/(.*)",
      "dest": "/api/ledger"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/tecp-reference-ui/dist/$1"
    }
  ]
}